               //$(".submitbooking").addClass("invi");
               //$(".submitbooking").hide();
               
               
                $(".div-capt-body").addClass('ratesnotactive');
                $(".book-remarks").addClass('ratesnotactive');
               
           }
           
           
           
    }
    
    
    
    
