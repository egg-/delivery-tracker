          if($(this).is(':visible')){
                buttonStatus=false;
                
          }else{
                 
          }
    });   
   
    
    if(!buttonStatus){
            
       $("#option-send-package").addClass('invi');
    }else{
        
       $("#option-send-package").removeClass('invi');
    }
    
 }
    
    
    
