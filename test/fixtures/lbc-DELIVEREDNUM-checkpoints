						method:"POST",
						dataType: 'json',
						url:"/ABookingAPI/processBookingIntl",
						data:$("#formInternationalBooking").serialize(),
						success: function(data) {
                                                    
                                                    if(data.process){
                                                         $('.nav-book-stick').css("display", "none");
