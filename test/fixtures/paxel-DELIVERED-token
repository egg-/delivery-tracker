			var u = arguments.length,

				n = arguments[0] || {},

				t, i, r;

			for (typeof n != "object" && typeof n != "function" && (n = {}), u == 1 && (n = this, t--), t = 1; t < u; t++) {

				i = arguments[t];

				for (r in i) Object.prototype.hasOwnProperty.call(i, r) && (n[r] = i[r])

			}

			return n

		},

		i = function(n) {

			return typeof n == "function" && typeof n.nodeType != "number"

		},

		t = function(n, i) {

			this.$element = n;

