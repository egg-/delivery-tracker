	};

	n.initDOM = function() {

		var n = function(n, t) {

				var i = document.createElement(n);

				return i.className = t, i

			},

			v = function(n, t) {

				var i = document.createElement("canvas");

				return i.width = n, i.height = t, i

			},

			f = v(this.options.width - 2, this.options.height),

			e = f.cloneNode(!0),

