<script>

var autocompletePickup, autocompleteDestination;

function currencyFormat(num) {

    return 'Rp ' + num.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1.')

  }



var captcha_check_rates = sliderCaptcha({

            id: 'captcha-check-rates',

            setSrc: function () {

              var randomInt = setInterval(function () {

                    var randomNumber = "3";

                }, 100);

                return "https://paxel.co/images/captcha/Img_3.png";

            },

            onSuccess: function () {

              var handler = setTimeout(function () {

                    window.clearTimeout(handler);

                    captcha_check_rates.reset();

                    $(".hide-captcha-check-rates").slideUp(500);

                    $("#validation-value").val("pass");

                    $("#check-rates-button").click();

                }, 500);

            }

        });

var captcha_track = sliderCaptcha({

            id: 'captcha-track',

            setSrc: function () {

              var randomInt = setInterval(function () {

                    var randomNumber = "5";

                }, 100);

                return "https://paxel.co/images/captcha/Img_5.png";

            },

            onSuccess: function () {

                var handler = setTimeout(function () {

                    window.clearTimeout(handler);

                    captcha_track.reset();

                    $(".hide-captcha-track").slideUp(500);

                    $("#trackshipment-button").click();

                }, 500);

            }

        });



function validationCheckRates() {

  if ($("#validation-value").val() == "pass") {

    return true

  }

  return false;

}



function initAutocomplete() {



  var options = {

    componentRestrictions: {

      country: 'id'

    }

  };



  var inputPickup = document.getElementById('pickup');

  autocompletePickup = new google.maps.places.Autocomplete(inputPickup, options);



  var inputDestination = document.getElementById('destination');

  autocompleteDestination = new google.maps.places.Autocomplete(inputDestination, options);



  autocompletePickup.addListener('place_changed', fillInAddressPickup);

  autocompleteDestination.addListener('place_changed', fillInAddressDestination);



  selectFirstWithoutPickedAddress(inputPickup);

  selectFirstWithoutPickedAddress(inputDestination);



  function selectFirstWithoutPickedAddress(input) {

    var _addEventListener = (input.addEventListener) ? input.addEventListener : input.attachEvent;

    function addEventListenerWrapper(type, listener) {

      if (type == "keydown") {

        var orig_listener = listener;

        listener = function (event) {

          var suggestion_selected = $(".pac-item-selected").length > 0;

          if ((event.which == 13 || event.which == 9 || event.which == 0) && !suggestion_selected) {

            var simulated_downarrow = $.Event("keydown", { keyCode:40, which:40 });

            orig_listener.apply(input, [simulated_downarrow]);

          }

          orig_listener.apply(input, [event]);

        };

      }

      _addEventListener.apply(input, [type, listener]);

    }

    if (input.addEventListener) {

      input.addEventListener = addEventListenerWrapper;

    } else if (input.attachEvent) {

      input.attachEvent = addEventListenerWrapper;

    }

  }



  function fillInAddressPickup() {

    var place = autocompletePickup.getPlace();

    var addressPickup = '';

    var zipCodePickup = '';

    var addressPickupName = '';

    var formatedAddressPickup = '';

    var cityPickup = '';

    var provincePickup = '';

    var kecamatanPickup = '';

    var kelurahanPickup = '';

    var latPickup = place.geometry.location.lat();

    var lngPickup = place.geometry.location.lng();

    var status = '';



    for (var i = 0; i < place.address_components.length; i++) {



      if(place.name){

        addressPickupName = place.name;

      }

      if(place.formatted_address){

        formatedAddressPickup = place.formatted_address;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_1') {

        provincePickup = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'route') {

        addressPickup = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_2') {

        cityPickup = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_4') {

        kelurahanPickup = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_3') {

        kecamatanPickup = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'postal_code') {

        zipCodePickup = place.address_components[i].long_name;

      }

    }



    var pickupData = {

      "address": addressPickup,

      "city": cityPickup,

      "kecamatan": kecamatanPickup,

      "kelurahan": kelurahanPickup,

      "postal_code": zipCodePickup,

      "lng": lngPickup.toString(),

      "lat": latPickup.toString()

    }



    $('#tooltip-form-validation-pickup').removeClass('show-component');

    $('#tooltip-form-validation-pickup').addClass('hide-component');

    $("#pickup-field").removeClass('has--incomplete');



    $.ajaxSetup({

        headers: {

            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

        }

    });

    $.ajax({

      url: "https://paxel.co/api/v1/zipcode-by-address",

      type: "POST",

      dataType: "json",

      data: pickupData,

      success: function(data) {

        if (data.status) {

          $("#zipcode-pickup").val(data.zipcode);

          $('#trackest-tooltip-pickup').removeClass('show-component');

          $('#trackest-tooltip-pickup').addClass('hide-component');

          $("#pickup-field").removeClass('has--incomplete');

          senderInformation.sender_address.label = addressPickupName;

          senderInformation.sender_address.name = formatedAddressPickup;

          senderInformation.sender_address.longitude = lngPickup.toString();

          senderInformation.sender_address.latitude = latPickup.toString();

          senderInformation.sender_address.province = provincePickup;

          senderInformation.sender_address.regency = kecamatanPickup;

          senderInformation.sender_address.district = kelurahanPickup;

          senderInformation.sender_address.zip_code = data.zipcode;

          senderInformation.sender_address.status = data.status;

        } else {

          $('#trackest-tooltip-pickup').removeClass('hide-component');

          $('#trackest-tooltip-pickup').addClass('show-component');

          $('#pickup').val('');

          $('#pickup').focus();

          $("#pickup-field").addClass('has--incomplete');

        }

      },

      error: function(err) {

        console.log("cannot to validate ", err);

      }

    });

  }



  function fillInAddressDestination() {

    var place = autocompleteDestination.getPlace();

    var addressDestination = '';

    var zipCodeDestination = '';

    var formatedAddressDestination = '';

    var addressDestinationName = '';

    var provinceDestination= '';

    var cityDestination = '';

    var kecamatanDestination = '';

    var kelurahanDestination = '';

    var latDestination = place.geometry.location.lat();

    var lngDestination = place.geometry.location.lng();

    var status = '';

    var zipCodeDestination = '';



    for (var i = 0; i < place.address_components.length; i++) {

      if(place.name){

        addressDestinationName = place.name;

      }

      if(place.formatted_address){

        formatedAddressDestination = place.formatted_address;

      }

      if (place.address_components[i].types[0] == 'route') {

        addressDestination = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_1') {

         provinceDestination = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_2') {

        cityDestination = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_3') {

        kecamatanDestination = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'administrative_area_level_4') {

        kelurahanDestination = place.address_components[i].long_name;

      }

      if (place.address_components[i].types[0] == 'postal_code') {

        zipCodeDestination = place.address_components[i].long_name;

      }

    }



    var destinationData = {

      "address": addressDestination,

      "city": cityDestination,

      "kecamatan": kecamatanDestination,

      "kelurahan": kelurahanDestination,

      "postalCode": zipCodeDestination,

      "lng": lngDestination.toString(),

      "lat": latDestination.toString()

    }



    $('#tooltip-form-validation-destination').removeClass('show-component');

    $('#tooltip-form-validation-destination').addClass('hide-component');

    $("#destination-field").removeClass('has--incomplete');



    $.ajaxSetup({

        headers: {

            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

        }

    });

    $.ajax({

      url: "https://paxel.co/api/v1/zipcode-by-address",

      type: "POST",

      dataType: "json",

      data: destinationData,

      success: function(data) {

        if (data.status) {

          $("#zipcode-destination").val(data.zipcode);

          $('#trackest-tooltip-destination').removeClass('show-component');

          $('#trackest-tooltip-destination').addClass('hide-component');

          $("#destination-field").removeClass('has--incomplete');

          receiverInformation.receiver_address.label = addressDestinationName;

          receiverInformation.receiver_address.name = formatedAddressDestination;

          receiverInformation.receiver_address.longitude = lngDestination.toString();

          receiverInformation.receiver_address.latitude = latDestination.toString();

          receiverInformation.receiver_address.province = provinceDestination;

          receiverInformation.receiver_address.regency = kecamatanDestination;

          receiverInformation.receiver_address.district = kelurahanDestination;

          receiverInformation.receiver_address.status = data.status;

          receiverInformation.receiver_address.zip_code = data.zipcode;

        } else {

          $('#trackest-tooltip-destination').removeClass('hide-component');

          $('#trackest-tooltip-destination').addClass('show-component');

          $('#destination').val('');

          $('#destination').focus();

          $("#destination-field").addClass('has--incomplete');

        }

      },

      error: function(err) {

        console.log("cannot to validate ");

      }

    });

  }

}



$(document).ready(function () {



  var currentHash = "#main-slide"

    $(document).scroll(function () {

        $('section').each(function () {

            var top = window.pageYOffset;

            var distance = top - $(this).offset().top;

            var hash = $(this).attr('id');



            if (distance < 50 && distance > -50 && currentHash != hash) {

              if(history.pushState) { history.pushState(null, null, "#"+hash); } else { window.location.hash = "#"+hash; }



                currentHash = hash;

            }

        });

    });







  if(""){

    var shipment = $('#track-shipment');

    $('html').scrollTop(shipment.offset().top) ;

  }



  if(""){

    var result = $('#check-rates');

    $('html').scrollTop(result.offset().top) ;

  }



  if(""){

    var shipment = $('#track-shipment');

    $('html').scrollTop(shipment.offset().top) ;

  }



  if(""){

    var result = $('#check-rates');

    $('html').scrollTop(result.offset().top) ;

  }



  var headersLength = 4;

  if (headersLength == 0) {

    $("#navbar").removeClass("has-main-slide");

    $("#navbar").addClass("no-main-slide");

  }



  $(".opt-toggle").click(function(){

    $(".opt").slideToggle('fast');

  })



  $(".opt tr").click(function(){

    var sizeType = $(this).find("span").text();

    $(".opt").slideToggle('fast');

    if($.trim(sizeType) != "Custom"){

      var sizeType = sizeType + " " + $(this).find("note").text();

    }



    var sizeValue = $(this).data("value");

    $("#parcel-size").val(sizeValue);

    $(".opt-toggle .selected").html(sizeType);

    $("#tooltip-form-validation-parcel-size").removeClass('show-component');

    $("#tooltip-form-validation-parcel-size").addClass('hide-component');

    $("#parcel-size-field").removeClass('has--incomplete');

  });



  $("#refresh-captcha-check-rates").click(function() {

    captcha_check_rates.reset();

  });

  $("#refresh-captcha-track").click(function() {

    captcha_track.reset();

  });

  $("#track-captcha").click(function() {

    var awb = $('#awb-text-area').val();

    if (awb == "") {

      $('#tooltip-form-validation-awb-field').removeClass('hide-component');

      $('#tooltip-form-validation-awb-field').addClass('show-component');

      $("#awb-text-area-card").addClass('error-text-awb');

      return;

    }

    $('#tooltip-form-validation-awb-field').addClass('hide-component');

    $('#tooltip-form-validation-awb-field').removeClass('show-component');

    $("#awb-text-area-card").removeClass('error-text-awb');

    $(".hide-captcha-track").slideDown(500);

  });

  $("#check-rates-captcha").click(function() {

    var pickup = $('#pickup').val();

    var destination = $('#destination').val();

    var weight = $("#weight").val();

    if (pickup == "" && destination =="" && (weight == "" || weight <= 0)) {

      $('#tooltip-form-validation-pickup').removeClass('hide-component');

      $('#tooltip-form-validation-pickup').addClass('show-component');

      $("#pickup-field").addClass('has--incomplete');

      $('#tooltip-form-validation-destination').removeClass('hide-component');

      $('#tooltip-form-validation-destination').addClass('show-component');

      $("#destination-field").addClass('has--incomplete');

      $('#tooltip-form-validation-weight').removeClass('hide-component');

      $('#tooltip-form-validation-weight').addClass('show-component');

      $("#weight-field").addClass('has--incomplete');

      return;

    }



    if (pickup == "" && destination =="") {

      $('#tooltip-form-validation-pickup').removeClass('hide-component');

      $('#tooltip-form-validation-pickup').addClass('show-component');

      $("#pickup-field").addClass('has--incomplete');

      $('#tooltip-form-validation-destination').removeClass('hide-component');

      $('#tooltip-form-validation-destination').addClass('show-component');

      $("#destination-field").addClass('has--incomplete');

      return;

    }

    if (pickup == "" && (weight == "" || weight <= 0)) {

      $('#tooltip-form-validation-pickup').removeClass('hide-component');

      $('#tooltip-form-validation-pickup').addClass('show-component');

      $("#pickup-field").addClass('has--incomplete');

      $('#tooltip-form-validation-weight').removeClass('hide-component');

      $('#tooltip-form-validation-weight').addClass('show-component');

      $("#weight-field").addClass('has--incomplete');

      return;

    }



    if (destination == "") {

      $('#tooltip-form-validation-destination').removeClass('hide-component');

      $('#tooltip-form-validation-destination').addClass('show-component');

      $("#destination-field").addClass('has--incomplete');

      return;

    }



    if (weight == "" || weight <= 0) {

      $('#tooltip-form-validation-weight').removeClass('hide-component');

      $('#tooltip-form-validation-weight').addClass('show-component');

      $("#weight-field").addClass('has--incomplete');

      return;

    }

    $(".hide-captcha-check-rates").slideDown(500);

  });



  $('.repeater').repeater({

    isFirstItemUndeletable: true,

    show: function () {

      if( $(this).parents(".repeater").attr("data-limit").length > 0 ){

        if( $(this).parents(".repeater").find("div[data-repeater-item]").length <= $(this).parents(".repeater").attr("data-limit") ){

          $(this).slideDown();

          $(this).find('input').removeAttr('required');

        } else {

          $(this).remove();

        }

      } else {

        $(this).slideDown();

      }

    },

    hide: function (deleteElement) {

      $(this).slideUp(deleteElement);

    }

  });



  function currencyFormat(num) {

    return 'Rp ' + num.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1.')

  }

  $('#guest-app').click( function(e) {

      window.localStorage.clear()

      window.localStorage.setItem('senderInformation',JSON.stringify(senderInformation));

      window.localStorage.setItem('receiverInformation',JSON.stringify(receiverInformation));

      window.location.href="https://paxel.co/id/guest-app/sender";

  });

});

</script>



<script>

  // add destination handler

  let destinationCounter = 0;

  $('#destination-counter').val(destinationCounter);

  $('#add-destination').click(function () {

    if (destinationCounter == 2) {

      return false;

    }



    let rows = "";

    rows += `<div class="count-${destinationCounter} input-group mb-1">`;

    rows += `<div class="input-group-prepend">`;

    rows += `<span class="input-group-text">`;

    rows += `<img loading="lazy" src="https://paxel.co/images/icon/oval_red.png" alt="Lokasi Tujuan">`;

    rows += `</span>`;

    rows += `</div>`;

    rows += `<input id="destination-${destinationCounter}" type="text" class="form-control" name="additional_destination[]" placeholder="Tujuan" aria-label="Enter Destination" aria-describedby="basic-addon1">`;

    rows += `<input type="hidden" id="zipcode-destination-${destinationCounter}" name="additional_zipcode_destination[]" class="form-control no-spin" value="" placeholder="Enter Zip Code Destination" />`;

    rows += `<div class="input-group-append">`;

    rows += `<span class="input-group-text">`;

    rows += `<img loading="lazy" src="https://paxel.co/images/icon/track.png" alt="Lokasi Tujuan">`;

    rows += `</span>`;

    rows += `</div>`;

    rows += `<div class="trackship_est_tooltip_home hide-component" id="trackest-tooltip-destination-${destinationCounter}">`;

    rows += `<p class="title">Lokasi Diluar Jangkauan</p>`;

    rows += `</div>`;

    rows += `<div class="trackship_est_tooltip_home hide-component" id="tooltip-form-validation-destination-${destinationCounter}">`;

    rows += `<p class="title">Tolong Isi Field</p>`;

    rows += `</div>`;

    rows += `</div>`;



    // append new field destination

    $('#destination-wrapper').append(rows);



    // init autocomplete for new field destination

    initAddDestinationAutoComplete(

      destinationCounter,

      `destination-${destinationCounter}`,

      `zipcode-destination-${destinationCounter}`,

      `trackest-tooltip-destination-${destinationCounter}`,

      `tooltip-form-validation-destination-${destinationCounter}`);



    // increment field destination

    destinationCounter++;

    $('#destination-counter').val(destinationCounter);



    if (destinationCounter == 2) {

      if (!$('#add-destination').hasClass('hide-component')) {

        $('#add-destination').addClass('hide-component')

      }

    }

  });



  // function for init autocomplete add destination

  let autocompleteAddDestinationWrapper = {};

  function initAddDestinationAutoComplete(counter, inputId, inputZipcodeId, toolTipOOCId, toolTipValidationId) {

    autocompleteAddDestinationWrapper["options_" + counter] = {

      componentRestrictions: {

        country: 'id'

      }

    };



    autocompleteAddDestinationWrapper["inputAddDestination_" + counter] = document.getElementById(inputId);

    autocompleteAddDestinationWrapper["autocompleteAddDestination_" + counter] = new google.maps.places.Autocomplete(autocompleteAddDestinationWrapper["inputAddDestination_" + counter], autocompleteAddDestinationWrapper["options_" + counter]);

    autocompleteAddDestinationWrapper["fillInAddressAddDestination_" + counter] = fillInAddressAddDestination;

    autocompleteAddDestinationWrapper["autocompleteAddDestination_" + counter].addListener('place_changed', fillInAddressAddDestination);

    selectFirstWithoutPickedAddressAddDestination(autocompleteAddDestinationWrapper["inputAddDestination_" + counter]);



    function selectFirstWithoutPickedAddressAddDestination(input) {

      var _addEventListener = (input.addEventListener) ? input.addEventListener : input.attachEvent;

      function addEventListenerWrapper(type, listener) {

        if (type == "keydown") {

          var orig_listener = listener;

          listener = function (event) {

            var suggestion_selected = $(".pac-item-selected").length > 0;

            if ((event.which == 13 || event.which == 9 || event.which == 0) && !suggestion_selected) {

              var simulated_downarrow = $.Event("keydown", { keyCode:40, which:40 });

              orig_listener.apply(input, [simulated_downarrow]);

            }

            orig_listener.apply(input, [event]);

          };

        }

        _addEventListener.apply(input, [type, listener]);

      }

      if (input.addEventListener) {

        input.addEventListener = addEventListenerWrapper;

      } else if (input.attachEvent) {

        input.attachEvent = addEventListenerWrapper;

      }

    }



    function fillInAddressAddDestination() {

      var placeAddDestination = autocompleteAddDestinationWrapper["autocompleteAddDestination_" + counter].getPlace();

      var addressAddDestination = '';

      var zipCodeAddDestination = '';

      var formatedAddressAddDestination = '';

      var addressAddDestinationName = '';

      var provinceAddDestination= '';

      var cityAddDestination = '';

      var kecamatanAddDestination = '';

      var kelurahanAddDestination = '';

      var latAddDestination = placeAddDestination.geometry.location.lat();

      var lngAddDestination = placeAddDestination.geometry.location.lng();

      var status = '';



      for (var i = 0; i < placeAddDestination.address_components.length; i++) {

        if(placeAddDestination.name){

          addressAddDestinationName = placeAddDestination.name;

        }

        if(placeAddDestination.formatted_address){

          formatedAddressAddDestination = placeAddDestination.formatted_address;

        }

        if (placeAddDestination.address_components[i].types[0] == 'route') {

          addressAddDestination = placeAddDestination.address_components[i].long_name;

        }

        if (placeAddDestination.address_components[i].types[0] == 'administrative_area_level_1') {

          provinceAddDestination = placeAddDestination.address_components[i].long_name;

        }

        if (placeAddDestination.address_components[i].types[0] == 'administrative_area_level_2') {

          cityAddDestination = placeAddDestination.address_components[i].long_name;

        }

        if (placeAddDestination.address_components[i].types[0] == 'administrative_area_level_3') {

          kecamatanAddDestination = placeAddDestination.address_components[i].long_name;

        }

        if (placeAddDestination.address_components[i].types[0] == 'administrative_area_level_4') {

          kelurahanAddDestination = placeAddDestination.address_components[i].long_name;

        }

        if (placeAddDestination.address_components[i].types[0] == 'postal_code') {

          zipCodeAddDestination = placeAddDestination.address_components[i].long_name;

        }

      }



      var destinationData = {

        "address": addressAddDestination,

        "city": cityAddDestination,

        "kecamatan": kecamatanAddDestination,

        "kelurahan": kelurahanAddDestination,

        "postalCode": zipCodeAddDestination,

        "lng": lngAddDestination.toString(),

        "lat": latAddDestination.toString()

      }



      $("#"+toolTipValidationId).removeClass('show-component');

      $('#'+toolTipValidationId).addClass('hide-component');

      // $("#destination-field").removeClass('has--incomplete');



      $.ajaxSetup({

          headers: {

              'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

          }

      });

      $.ajax({

        url: "https://paxel.co/api/v1/zipcode-by-address",

        type: "POST",

        dataType: "json",

        data: destinationData,

        success: function(data) {

          if (data.status) {

            $("#"+inputZipcodeId).val(data.zipcode);

            $('#'+toolTipOOCId).removeClass('show-component');

            $('#'+toolTipOOCId).addClass('hide-component');

          } else {

            $('#'+toolTipOOCId).removeClass('hide-component');

            $('#'+toolTipOOCId).addClass('show-component');

            $('#'+inputId).val('');

            $('#'+inputId).focus();

          }

        },

        error: function(err) {

          console.log("cannot to validate ");

        }

      });

    }

  }



  $("#awb-text-area").on("keyup", function(){

    trackSubmitButton();

  });



  $("#weight").on("keyup", function(){

    checkRatesSubmitButton();

    if ($(this).val() <= 0 || $(this).val() > 25) {

      $(this).val(null);

      return false;

    }

  });



  $("#pickup").on("keyup", function(){

    checkRatesSubmitButton();

  });



  $("#destination").on("keyup", function(){

    checkRatesSubmitButton();

  });



  function checkField(value) {

    return value != "" && value != null

  }



  function checkRatesSubmitButton() {

    if(checkField($("#weight").val()) &&  checkField($("#pickup").val()) && checkField($("#destination").val())){

      if ($("#check-rates-captcha").hasClass("disabled")) {

        $("#check-rates-captcha").removeClass("disabled");

      }

    } else {

      if (!$("#check-rates-captcha").hasClass("disabled")) {

        $("#check-rates-captcha").addClass("disabled");

      }

    }

  }



  function trackSubmitButton() {

    if(checkField($("#awb-text-area").val())){

      if ($("#track-captcha").hasClass("disabled")) {

        $("#track-captcha").removeClass("disabled");

      }

    } else {

      if (!$("#track-captcha").hasClass("disabled")) {

        $("#track-captcha").addClass("disabled");

      }

    }

  }



</script>

