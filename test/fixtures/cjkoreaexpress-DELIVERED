<script type="text/javascript">
var sn = '';
var sn1 = '';
function SET_GBN(menu1, menu2){
	var $menu1 = $("div.gnb").find("#gnb_" + menu1);
	if($menu1.length != 0){
		$menu1.removeClass("default").addClass("focus");
		//$menu1.attr("data-set", "1");
		var $menu2 = $("div.gnb").find("#gnb_" + menu1).find("#gnb_sub_"+ menu1 +"_" + menu2);
		if($menu2.length != 0){
			$menu2.attr("class", "focus");
		}
	}

	if(sn == '0' || sn == '7' || sn == '12'){
		if ($("div.wrap").hasClass("depth2_focus")){
			$("div.wrap").removeClass("depth2_focus");
			$("div.wrap").addClass("depth2_default");
		}
	}

};

function BIND_GNB_EVENT(){
	var $menus = $("div.gnb>div");
	$menus.each(function(){
		$(this).mouseover(function(){
			if ($("div.wrap").hasClass("depth2_default")){
				$("div.wrap").removeClass("depth2_default");
				$("div.wrap").addClass("depth2_focus");
			}
			$menus.removeClass("focus");
			$menus.removeClass("default");
			$menus.addClass("default");
			$(this).removeClass("default");
			$(this).addClass("focus");
		});
	});

	$menus.find("a.depth1").each(function(){
		$(this).focus(function(){
			var $parent = $(this).parents("div:first");
			$parent.trigger("mouseover");
		});
	});

	$("#gng_area_out").mouseout(function(){
		$menus.removeClass("focus");
		$menus.removeClass("default");
		$menus.addClass("default");
		SET_GBN(sn, sn1);
	});
};

var SKIP_NAVI_EVENT_BIND = function(){

	var $skipNavi = $("div.skipnavi");
	var $skipNavip = $("div.skipnavi").find("p");

	var $dumy = $("div.dumySkipNavi");

	$dumy.find("a").focus(function(){
		$skipNavi.show();
		$skipNavip.hide();
		var cnt = $(this).attr("tabindex");
		$("div.skipnavi").find("p:nth-child("+ cnt +")").show();
	});

	$dumy.find("a").blur(function(){
		$skipNavi.hide();
	});
};

function chatPop() {
	window.open("https://chatbot.doortodoor.co.kr/chatbot/chatbot.jsp?company=0000000000&media=COM",  "scrollbars=yes", "width=800, height=1500");
};

$(document).ready(function(){
	SKIP_NAVI_EVENT_BIND();
	BIND_GNB_EVENT();

	sn = '1';
	sn1 = '05';

	SET_GBN(sn, sn1);


});

var onOneClickFlag = true;

function toggleOneClickService(){
	var $bt_onclick = $("#bt_onclick");

	if($bt_onclick.hasClass("default")){


		if($("#onclickContainner").find("iframe").length == 0){
			$("#onclickContainner").append('<iframe src="/onclick/index.jsp" style="width:1106px; height:475px; margin-left:52px; margin-top:-23px;" frameborder="0" title="원클릭서비스"></iframe>');
		}

		$bt_onclick.removeClass("default");
		$bt_onclick.addClass("focus");
		$("#onclickContainner").show();
	}
	else{
		$bt_onclick.removeClass("focus");
		$bt_onclick.addClass("default");
		$("#onclickContainner").hide();
	}
}

function onOneClickService(){
	if(onOneClickFlag){
		toggleOneClickService();
	}
	onOneClickFlag = true;
}

function onFocusOneClickService(obj){
	toggleOneClickService(obj);
}

function onblurOneClickService(obj){
	onOneClickFlag = false;
}

$(function(){
	$(window).scroll(function() {
		var t = $(window).scrollTop() - 100;
		if(t < 51){
			t = 51;
		}
		$("div.oneclick_area").stop();
		$("div.oneclick_area").animate({'top': t }, 51);


		var v = $(window).scrollTop() - 23;
		if(v < 128){
			v = 128;
		}
		$("div.oneclick_area2").stop();
		$("div.oneclick_area2").animate({'top': v }, 128);
	});
});

</script>

