            <script type="text/javascript">
            function onShowSelect(obj){
            	if( $("#" + obj).hasClass("focus") ){
            		$("#" + obj).removeClass("focus");
            		$("#" + obj).find(".site_area").hide();
            	}
            	else{
            		$("#" + obj).addClass("focus");
            		$("#" + obj).find(".site_area").show();
            	}
            }

            function onLinkSelect(obj, url){
            	window.open(url,"");
            	onShowSelect(obj);
            }
            </script>
