	if($("div.popup_wrap").length > 0){
		var $table = $("div.popup_wrap").find("table");

		$table.find("tr").each(function(){
			if(!($(this).hasClass("h33"))){
				$(this).addClass("h33");
			}
		});

		$table.find("th").each(function(){
			if(!($(this).hasClass("h33"))){
				$(this).addClass("h33");
			}
		});

		$table.find("td").each(function(){
			if(!($(this).hasClass("h33"))){
				$(this).addClass("h33");
			}
		});
	}
});

