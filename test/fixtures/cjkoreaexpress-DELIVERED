<script type="text/javascript">
	function SHOW_ALL_MENU(mode){
		if(mode == 1){
			$("div.allmenu_area").removeClass("default");
			$("div.allmenu_area").addClass("focus");
		}
		else{
			$("div.allmenu_area").removeClass("focus");
			$("div.allmenu_area").addClass("default");
		}
	}

	function allMenuFocusMove(){
		try{
			document.getElementById("allMenuTab1").focus();
		}
		catch(e){

		}
	}

	function chatPop() {
		window.open("https://chatbot.doortodoor.co.kr/chatbot/chatbot.jsp?company=0000000000&media=COM",  "scrollbars=yes", "width=800, height=1500");
	}

</script>

