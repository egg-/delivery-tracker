<script type="text/javascript">
	$(function()  {
		var cdwDialog;
		var diDialog;
		var srCancelDialog;
		var dcrDialog;
		var TrackingModal;
	});

	function myChoiceCallBackFunction(dialogName, statusCode){

		if (dialogName == "qvDialog"){
			qvDialog.dialog('close');
			qvDialog.remove();
		}
		else if (dialogName == "cdwDialog"){
			cdwDialog.dialog('close');
			cdwDialog.remove();
		}
		else if (dialogName == "diDialog"){
			diDialog.dialog('close');
			diDialog.remove();
		}
		else if (dialogName == "srCancelDialog"){
			srCancelDialog.dialog('close');
			srCancelDialog.remove();
		}
		else if (dialogName == "DCO"){
			dcrDialog.dialog('close');
			dcrDialog.remove();
		}
		if ((statusCode != "null" || statusCode != "") && statusCode == "00"){
			if (dialogName == "diDialog"){
				$("#asr").css("display","none");
				$("#csr").css("display","block");
			}
			if (dialogName == "srCancelDialog"){
				$("#asr").css("display","block");
				$("#csr").css("display","none");
			}
			if (dialogName == "DCO" || dialogName == "diDialog" || dialogName == "cdwDialog" || dialogName == "srCancelDialog")
			{
				redirectdetDCO('');
				}
			else
			{
			  	var url = window.location.href;
				var ctaT = "null";

				if(ctaT == "null")
				    location.reload(true);
				else
				{

				     var alturl = url;
				     if(url.search("type=") != -1)
				     {
				       alturl = url.replace("type=" + ctaT, "type=" );
				     }


				     window.location.href = alturl;
				}
			}
		}
		return false;
	}

	function popupSII(count){
      	window.scroll(0,50);
	  	var width = 970;
	  	siiDialog=$('<div class="VB_Dialog appLvl mod modal dialog"></div>').html("").dialog({autoOpen:false,modal:true,bgiframe:true,width:970,position:"center",dialogClass:'modal'});
	  	var sii_form = document.getElementById("siiformid"+count);
      	var URL =  $(sii_form).attr("action");
      	var params = $(sii_form).serialize();
	  	siiDialog.load(URL+"?"+params, function(){siiDialog.dialog('open');} );
	  	$('.ui-dialog').css("top", "72");
	  	$('.ui-dialog').css("overflow","visible");
	  	$('.ui-dialog-titlebar').css("display","none");
      	var adjustedWidth = screen.width * 0.05;
      	$('.ui-dialog').css("left",adjustedWidth);
      	$('.ui-dialog').css('z-index',103);
        $('.ui-widget-overlay').css('z-index',102);
        $('.ui-widget-overlay').css("position","fixed");
	  	return false;
	}

	function popupSIIfromAIA(){
		window.scroll(0,50);
     	var URL = "/sii/wtrack?" + "request_sii=1&loc=en_KR&ReturnToURL=/sii/wtrack&Requester=SII&KeepThis=true&id=aia";
      	siiDialog.load(URL, function(){siiDialog.dialog('open');} );
		$('.ui-dialog').css("width", "970");
		$('.ui-dialog').css("top", "72");
		siiDialog.dialog( 'option', 'position', 'center' );
      	$('.ui-dialog').css("overflow","visible");
      	$('.ui-dialog-titlebar').css("display","none");
      	var adjustedWidth = screen.width * 0.05;
      	$('.ui-dialog').css("left",adjustedWidth);
      	$('.ui-dialog').css('z-index',103);
        $('.ui-widget-overlay').css('z-index',102);
        $('.ui-widget-overlay').css("position","fixed");
      	return false;
	}

	function aiaCompleted(){
    	popupSIIfromAIA();
      	return false;
    }

	function submitCOWForm(form){
	    form.submit();
	    return false;
 	}


	function podPopup1(count, id){
    	try{
			var pod_form = document.getElementById("podFormid"+count);
			var data;
			if(id == "retailStore")
			{
       		    data = "lineData=" + pod_form.retailLineData.value  + "&Requester=" + pod_form.Requester.value + "&refNumbers=" + pod_form.refNumbers.value + "&loc=" + pod_form.podLoc.value;
            }
            else
            {
                data = "Requester=" + pod_form.Requester.value + "&tracknum=" + pod_form.tracknum.value + "&refNumbers=" + pod_form.refNumbers.value + "&loc=" + pod_form.podLoc.value;
            }
            var p_url = pod_form.podURL.value;

          		printerFriend(p_url + "/processPOD?" + data);



      	}
      	catch(error){

      	}
	}


	function popupUpsell(count){
		window.scroll(0,50);
		upsellDialog=$('<div class="UPSELL_Dialog appLvl mod modal dialog"></div>').html("").dialog({autoOpen:false,modal:true,bgiframe:true,width:710,position:"center"});
        upsellDialog.dialog( "option", "resizable", false );
        var upsell_form = document.getElementById("upsellformid"+count);
        var URL =  $(upsell_form).attr("action");
        var params = $(upsell_form).serialize();
        if(count == "" || count == "null"){
        	params += "&detail_page=true";
        }
        var multi_form = document.getElementById("multiformid" + count);
        if(multi_form != null){
        	params += "&multi_page=true";
        }
		var dialogHTML = TrackingModal.retrieveDialogContents(URL, params);
		upsellDialog.html(dialogHTML);
		$('.ui-dialog').css("top", "72");
		$('.ui-dialog').css("overflow","visible");
		$('.ui-dialog-titlebar').css("display","none");
        var adjustedWidth = screen.width * 0.05;
        $('.ui-dialog').css("left",adjustedWidth);
        upsellDialog.dialog('open');
        $('.ui-dialog').css('z-index',103);
        $('.ui-widget-overlay').css('z-index',102);
        $('.ui-widget-overlay').css("position","fixed");
		return false;
	}


	function redirectdet(count){
			if(count == "" || count == "null"){
          	document.location.href="https://wwwapps.ups.com/tracksec/returnToDetails?loc=en_KR&dcrmodal=true";
        }
        else{
        	document.location.href="https://wwwapps.ups.com/tracksec/returnToSummary?loc=en_KR&dcrmodal=true&sel_index=" + count;
        }
        return false;
	}


	function redirectdetDCO(count){
    	if(count == "" || count == "null"){
        	document.location.href="https://wwwapps.ups.com/tracksec/returnToDetails?loc=en_KR";
        }
        else{
        	document.location.href="https://wwwapps.ups.com/tracksec/returnToSummary?loc=en_KR&sel_index=" + count;
        }
		return false;
	}

	function redirectsii(count){
        if(count == "" || count == "null"){
        	document.location.href="https://wwwapps.ups.com/tracksec/returnToDetails?loc=en_KR&siimodal=true";
        }
        else{
        	document.location.href="https://wwwapps.ups.com/tracksec/returnToSummary?loc=en_KR&siimodal=true&sel_index=" + count;
        }
		return false;
	}


	function popupMC( id, count ){

	  	 if(id == "mcdi")
		     {
		      TrackingModal.popupDeliveryInstructions(count);
		      }
		     else if(id == "dco")
		     {
		         TrackingModal.popupDCR(count, 'dcoformid');
		     }
		     else if(id == "cdw")
		     {
		         TrackingModal.popupTwoHourDeliveryWindow(count);
		      }
		      else if(id == "csr")
		     {
		         TrackingModal.popupCancelShipmentRelease(count);
		      }
		      else if(id == "asr")
		      {
		         TrackingModal.popupDeliveryInstructions(count);
		      }
		      else if(id == "qvn")
		      {

		    	  if(count!='null' && count!=""){
		  	 		var qvn_form = $('#statusUpdatesformid'+count);

		  	 	  }
			  	 	else{
			  	 		var qvn_form = $('#statusUpdatesformid');
			  	 	}
		    	  TrackingModal.popupDialogViaForm(qvn_form,  790);
		      }

        return false;

	}

	function returnBtn(event,url){
		event.preventDefault ?
		event.preventDefault() : (event.returnValue = false);
		window.location=url;
	}

		//ER7716
	function openPage(url) {
		  window.open(url, '_blank');

	}
</script>

</div>

































































