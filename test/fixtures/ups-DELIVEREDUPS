<script language="javascript" type="text/JavaScript">

var test = 0;

function doZoom(size) {

if ((size == '10') && (test == 0))
 {
    return;
 }
  var p = document.getElementById('fontControl').getElementsByTagName('*');


   for(i=0;i<p.length;i++) {

      if(p[i].tagName == "H4")
      {
          if(size == '8')
          {
            p[i].style.fontSize = 10 +"pt"
          }
          else if(size == '10')
   		  {
   		    p[i].style.fontSize = 12 +"pt"
   		  }
   		  else if(size == '12')
  		  {
             p[i].style.fontSize = 14 +"pt"
   		  }
      }
      else if((p[i].id == "tt_spStatus") || (p[i].id == "tt_ovntStatus")|| (p[i].id == "tt_pgfStatus") ||(p[i].id == "trkNum") )
      {
          if(size == '8')
          {
            p[i].style.fontSize = 14 +"pt"
          }
          else if(size == '10')
   		  {
   		    p[i].style.fontSize = 16 +"pt"
   		  }
   		  else if(size == '12')
  		  {
             p[i].style.fontSize = 18 +"pt"
   		  }
      }
      else if((p[i].id != "fontLarge") && (p[i].id != "fontMedium") && (p[i].id != "fontSmall"))
      {
       	p[i].style.fontSize = size +"pt"
      }


   }

   if(size == '8')
   {
      test =1;
      document.getElementById('fontSmall').style.backgroundColor ='#D8D8D8';
      document.getElementById('fontMedium').style.backgroundColor ='#F2F2F2';
      document.getElementById('fontLarge').style.backgroundColor ='#F2F2F2';
   }
   else if(size == '10')
   {
      document.getElementById('fontSmall').style.backgroundColor ='#F2F2F2';
      document.getElementById('fontMedium').style.backgroundColor ='#D8D8D8';
      document.getElementById('fontLarge').style.backgroundColor ='#F2F2F2';
   }
   else if(size == '12')
   {
     test = 1;
      document.getElementById('fontSmall').style.backgroundColor ='#F2F2F2';
      document.getElementById('fontMedium').style.backgroundColor ='#F2F2F2';
      document.getElementById('fontLarge').style.backgroundColor ='#D8D8D8';
   }

}

function podPopup(form)
    {
    	try
      	{

       		var data = "lineData=" + form.lineData.value  + "&Requester=" + form.Requester.value + "&refNumbers=" + form.refNumbers.value + "&loc=" + form.podLoc.value;


          		printerFriend("/WebTracking/processPOD?" + data);




      }
      catch(error)
      {

      }
    }

function popupDescription(trackingNumber)
	{
	    var desVal;
	    var p = document.getElementsByTagName('input');
	    var desName = "descValue" + trackingNumber;
	    for(i=0;i<p.length;i++)
	     {
	      if(p[i].name == desName)
	      {
	       	desVal = p[i].value;
	      }
	   }
    	 var URL1;
    	 if(desVal != "")
    	 {

    		URL1 = "&trackingNumber=" + trackingNumber + "&descStatus=Y&description=" + encodeURIComponent(desVal);

    	 }
    	 else
    	 {
    	    URL1 = "&trackingNumber=" + trackingNumber + "&descStatus=N";

    	 }

    	var URL = "/WebTracking/desc?loc=en_KR";
    	URL += URL1;

     	TrackingModal.popupDialog(URL, 560);

    	return false;
	}

</script>

