   	function redirectmycenrollCTA(){
   		window.location="https://www.ups.com/lasso/login?loc=en_KR&returnto=https://www.ups.com/WebTracking/mcRequest?loc=en_KR&type=null&tracknum=DELIVEREDUPS";
	}

	function popupCTA(ctaRe){
		window.scroll(0,50);
		ctaDialog=$('<div class="UPSELL_CTA_Dialog appLvl mod modal dialog"></div>').html("").dialog({autoOpen:false,modal:true,bgiframe:true,width:750,position:"center"});
        ctaDialog.dialog( "option", "resizable", false );
        var upsell_form = document.getElementById("changeDelBridgeFormid");
        var URL =  $(upsell_form).attr("action");
        var params = $(upsell_form).serialize();
         var pa1 = "&Requester=" + "UPSHome" + "&type=" + ctaRe;


		params += pa1;
		ctaDialog.load(URL, params);
		ctaDialog.dialog('open');
		$('.ui-dialog').css("top", "72");
		$('.ui-dialog').css("overflow","visible");
		$('.ui-dialog-titlebar').css("display","none");
        var adjustedWidth = screen.width * 0.05;
        $('.ui-dialog').css("left",adjustedWidth);
		return false;
	}



function loginComplete() {

	if(clickLink==2)
	 	window.location="https://www.ups.com/WebTracking/mcRequest?loc=en_KR&type=UDCO&tracknum=DELIVEREDUPS";
	else if (clickLink==3){

		window.location="https://wwwapps.ups.com/WebTracking/returnToSummary?loc=en_KR&siimodal=true&sel_index=null";
	} else if (clickLink==4){
		window.location="https://wwwapps.ups.com/WebTracking/returnToDetails?loc=en_KR&adrcrctnmodal=true";
	} else
		window.location="https://wwwapps.ups.com/WebTracking/returnToDetails?loc=en_KR";
}


function handleCTAReturns()
	{
	var ctbRe = "null";

	var selog = "false";
	var ctad = "NOT";

	var show_sii = document.getElementById("CTA_SII");
	var show_dco = document.getElementById("CTA_DCO");

	var show_qvd = document.getElementById("CTA_QVD");
	var show_asr = document.getElementById("CTA_MCDI");

	//Jul2015-RFC 7405: adding constants for CTA-NES Interface - type
	var chdl = "CHDL";
	var mdco = "MDCO";
	var mfd = "MFD";
	var nmfd = "NMFD";
	var masr = "MASR";
	var nmasr = "NMASR";
	var udco = "UDCO";

	 if(ctbRe == "null")
     {
     return;
     }

	if((ctbRe.search(chdl) != -1)  && ("true" == selog))
	{
	    ctbRe = mdco;
	    if(document.getElementById("upsellErrType") != null)
     	{
         document.getElementById("upsellErrType").value = mdco;
     	}
	}

	if(ctad == "NOT")
	  return;

		     if((ctbRe.search(chdl) != -1) && ("false" == selog) && (ctad != ""))
		     {
		        popupCTA(ctbRe);
		     }
		      else if ((mfd == ctbRe) || (nmfd == ctbRe))
		      {

		     if(show_sii != null) {

					 popupSII('');


		     }
		     else if(show_qvd != null)
		   {
				popupUpsell('');
		   }
		   else if(show_dco != null )
		     {
		       TrackingModal.popupDCR('', 'dcoformid');
	         }
	         else
	         {


	     	   redirectmycenrollCTA();

	         }
	         }
	         else if(mdco == ctbRe)
	         {

		       if(show_sii != null ) {

					 popupSII('');


		     }
		     else if(show_qvd != null)
		   {
				popupUpsell('');
			 }
	          else if(show_dco != null  )
		     {
		          popupMC('dco', '');


	         }else{


	     	   redirectmycenrollCTA();

	         }
	         }
	         else if((masr  == ctbRe) || (nmasr  == ctbRe))
	         {

		      if( show_asr != null ) {

		        popupMC('asr', '');
		     }
		     else if(show_qvd != null)
		   	{
				popupUpsell('');
			}
			else{



	     	   redirectmycenrollCTA();

	         }
	         }
	         else if(udco  == ctbRe)
	         {
	             if(show_dco != null  )
			     {
			       popupMC('dco', '');


		         }
	         }
	}



</script>


