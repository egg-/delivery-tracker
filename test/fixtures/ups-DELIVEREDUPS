<script language="javascript" type="text/JavaScript">

	$(document).ready(function() {



	     $('.module3 div.panel-body').toggle();
			 $('#collapse3 span').toggleClass('collapseIcon').toggleClass('expandIcon');
			 $('#collapse3').toggleClass('collapsed').toggleClass('');
			 $('.module4 div.secBody').toggle();
	     	 $('.module4 div.secBody').toggle();



	   $('#collapse3').click(function(){

	     $('.module3 div.panel-body').toggle();
           logProgress();

	     $('.module4 div.secBody').toggle();
	     $('.module4 div.secBody').toggle();

			 $('#collapse3 span').toggleClass('collapseIcon').toggleClass('expandIcon');
			 $('#collapse3').toggleClass('collapsed').toggleClass('');


	   });



 });

    function logProgress() {
        var xhttp;
        var trackNum = document.getElementById("statusUpdatesformid").trackNum.value;
        var loc = document.getElementById("statusUpdatesformid").loc.value;
        var params = "trackNum=" + trackNum + "&loc=" + loc;
        if (window.XMLHttpRequest) {
            xhttp = new XMLHttpRequest();
            }
        else {
            xhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
        xhttp.open("POST", "/WebTracking/shipmentProgress", true);
        xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        xhttp.send(params);

    }

</script>



















