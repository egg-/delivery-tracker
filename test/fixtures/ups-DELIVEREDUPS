	   <input type="hidden" name="mc_request_type" value="enroll">
	   <input type="hidden" name="trackingnumber" value="DELIVEREDUPS">
	   <input type="hidden" name="lineItem" value="line1">
	   <INPUT name="HIDDEN_FIELD_SESSION" type="HIDDEN" value="">
	   <input type="hidden" name="loc" value="en&#95;KR">
</form>



